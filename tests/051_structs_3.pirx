struct Vector {
  x: int;
  y: int;
}

func vector_add(v1: *Vector, v2: *Vector, out: *Vector) {
  (*out).x = (*v1).x + (*v2).x;
  (*out).y = (*v1).y + (*v2).y;
}

func vector_print(v: *Vector) {
  printf("(%d, %d)\n", (*v).x, (*v).y);
}

func main(): int {
  var v1: Vector;
  v1.x = 1;
  v1.y = 2;

  var v2: Vector;
  v2.x = 10;
  v2.y = 20;

  var res: Vector;
  vector_add(&v1, &v2, &res);

  vector_print(&v1);
  vector_print(&v2);
  vector_print(&res);

  return 0;
}
