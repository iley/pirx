extern func main(): int {
  var numbers = new([]*int, 10);

  for var i = 0; i < 10; i++ {
    numbers[i] = new(int);
    *(numbers[i]) = i + 1;
  }

  for i = 0; i < 10; i++ {
    printf("%d\n", *(numbers[i]));
  }

  for i = 0; i < 10; i++ {
    dispose(numbers[i]);
  }

  dispose(numbers);
  return 0;
}
