extern func main(): int {
  var numbers = new([]*int, 10);

  var i = 0;
  while i < 10 {
    numbers[i] = new(int);
    *(numbers[i]) = i + 1;
    i = i + 1;
  }

  i = 0;
  while i < 10 {
    printf("%d\n", *(numbers[i]));
    i = i + 1;
  }

  i = 0;
  while i < 10 {
    dispose(numbers[i]);
    i = i + 1;
  }

  dispose(numbers);
  return 0;
}
