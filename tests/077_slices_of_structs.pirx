struct Vec3 {
  x: int;
  y: int;
  z: int;
}

extern func main(): int {
  var vectors = new([]Vec3, 10);

  var n = 1;
  var i = 0;
  while i < 10 {
    vectors[i].x = n;
    vectors[i].y = n * 10;
    vectors[i].z = n * 100;
    n = n + 1;
    i = i + 1;
  }

  i = 0;
  while i < 10 {
    printf("(%d, %d, %d)\n", vectors[i].x, vectors[i].y, vectors[i].z);
    i = i + 1;
  }

  dispose(vectors);
  return 0;
}
