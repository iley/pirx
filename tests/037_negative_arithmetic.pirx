extern func main(): int {
  var a: int;
  var b: int;
  a = -10;
  b = 5;
  printf("a = %d, b = %d\n", a, b);
  printf("a + b = %d\n", a + b);
  printf("a - b = %d\n", a - b);
  printf("a * b = %d\n", a * b);
  printf("a / b = %d\n", a / b);
  printf("a %% b = %d\n", a % b);
  printf("-a = %d\n", -a);
  printf("-b = %d\n", -b);
  printf("-(a + b) = %d\n", -(a + b));
  return 0;
}