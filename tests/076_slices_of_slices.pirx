extern func main(): int {
  var nrows = 3;
  var ncols = 5;

  var matrix = new([][]int, nrows);
  var row = 0;
  while row < nrows {
    matrix[row] = new([]int, ncols);
    row = row + 1;
  }

  var n = 1;
  row = 0;
  while row < nrows {
    var col = 0;
    while col < ncols {
      matrix[row][col] = n;
      n = n + 1;
      col = col + 1;
    }
    row = row + 1;
  }

  row = 0;
  while row < nrows {
    var col = 0;
    while col < ncols {
      printf("%2d", matrix[row][col]);
      if col != ncols - 1 {
        printf(" ");
      }
      col = col + 1;
    }
    printf("\n");
    row = row + 1;
  }

  row = 0;
  while row < nrows {
    dispose(matrix[row]);
    row = row + 1;
  }

  dispose(matrix);
  return 0;
}
