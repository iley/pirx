struct Vec3 {
  x: int;
  y: int;
  z: int;
}

func main(): int {
  var vectors = new([]Vec3, 10);

  var n = 1;
  for var i = 0; i < 10; i++ {
    vectors[i].x = n;
    vectors[i].y = n * 10;
    vectors[i].z = n * 100;
    n++;
  }

  for var i = 0; i < 10; i++ {
    printf("(%d, %d, %d)\n", vectors[i].x, vectors[i].y, vectors[i].z);
  }

  dispose(vectors);
  return 0;
}
